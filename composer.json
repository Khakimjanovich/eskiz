{
    "name": "laravel-notification-channels/eskiz",
    "description": "Eskiz notification channel for Laravel",
    "homepage": "https://github.com/laravel-notification-channels/eskiz",
    "license": "MIT",
    "authors": [
        {
            "name": "Abduraxmanov Yunusali",
            "email": "yunusalikhakimjanovich@gmail.com",
            "homepage": "https://heey.uz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0",
        "illuminate/support": "~6.0 || ~7.0 || ~8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Eskiz\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Eskiz\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Eskiz\\EskizServiceProvider"
            ]
        }
    }
}
